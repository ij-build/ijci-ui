    "react-dom": "^16.11.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "tslint": "^5.20.0",
    "tslint-react": "^4.1.0",
    "typescript": "^3.6.4"
  },
  "devDependencies": {
    "@types/jest": "24.0.21",
    "@types/lodash": "^4.14.149",
    "@types/node": "12.12.5",
